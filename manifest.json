{
  "manifest_version": 2,
  "name": "Goplum",
  "description": "A Google Chrome extension that takes playing music from Google Play Music and puts it on a server.",
  "version": "1.0",
  "minimum_chrome_version": "10.0",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "96": "img/icon96.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "tabs", "activeTab", "webRequest", "*://24.125.232.31/", "*://play.google.com/*"
  ]
}
