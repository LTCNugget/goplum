{
  "manifest_version": 2,
  "name": "Goplum",
  "description": "A Google Chrome extension that takes playing music from Google Play Music and puts it on a server.",
  "version": "0.7",
  "minimum_chrome_version": "10.0",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "96": "img/icon96.png"
  },
  "content_scripts": [
    {
      "matches": ["https://play.google.com/music/listen?*"],
      "js": ["js/jQuery.min.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab", "webRequest", "*://24.125.232.31/", "*://play.google.com/*"
  ]
}
